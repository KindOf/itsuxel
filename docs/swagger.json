{
    "swagger": "2.0",
    "info": {
        "description": "Educational Excel-like API",
        "title": "ITSUXEL",
        "contact": {
            "email": "iostapovychweb@gmail.com"
        },
        "version": "1.0"
    },
    "host": "localhost:3000",
    "basePath": "/api/v1",
    "paths": {
        "/api/v1/table/{sheet}/{cell}": {
            "post": {
                "description": "Add a new pet to the store",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "adds value to cell",
                "parameters": [
                    {
                        "type": "string",
                        "description": "sheet name",
                        "name": "sheet",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "cell address",
                        "name": "cell",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}